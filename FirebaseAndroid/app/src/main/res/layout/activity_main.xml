        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/querytag2" />

    <Button
        android:id="@+id/querytag4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="104dp"
        android:text="query friend 生活"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/querytag1" />

    <Button
        android:text="query friend all"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/queryall"/>

    <Button
        android:id="@+id/queryalltag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
